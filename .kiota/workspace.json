{
  "version": "1.0.0",
  "clients": {
    "GitHubClient": {
      "language": "CSharp",
      "typeAccessModifier": "Public",
      "structuredMimeTypes": [
        "application/json",
        "text/plain;q=0.9",
        "application/x-www-form-urlencoded;q=0.2",
        "multipart/form-data;q=0.1"
      ],
      "clientNamespaceName": "DownstreamClients.GitHub",
      "usesBackingStore": false,
      "includeAdditionalData": true,
      "excludeBackwardCompatible": false,
      "disabledValidationRules": [],
      "descriptionLocation": "https://github.com/github/rest-api-description/raw/refs/heads/main/descriptions/api.github.com/api.github.com.json",
      "includePatterns": [
        "/repos/{owner}/{repo}/releases/*"
      ],
      "excludePatterns": [],
      "outputPath": "./BankApi.Core/DownstreamClients/GitHub"
    }
  },
  "plugins": {}
}